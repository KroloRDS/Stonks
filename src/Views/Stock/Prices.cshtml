@model IEnumerable<AvgPrice>

<h1>Prices</h1>
<table class="table">
	<thead>
		<tr>
			<th><b>DateTime</b></th>
			<th><b>Price</b></th>
		</tr>
	</thead>
	<tbody>
		@{
		foreach (var offer in Model)
		{
			<tr>
				<td>@Html.DisplayFor(modelItem => offer.DateTime)</td>
				<td>@Html.DisplayFor(modelItem => offer.Price)</td>
			</tr>
		}
	}</tbody>
</table>
